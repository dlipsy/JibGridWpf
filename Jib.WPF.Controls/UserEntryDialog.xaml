<Window x:Class="Jib.WPF.Controls.UserEntryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jib.WPF.Controls"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Enter Name" Height="128.872" Width="275">
    <StackPanel>
        <TextBox x:Name="ResponseTextBox" Margin="8" MaxLength="100" KeyDown="ResponseTextBox_KeyDown" />
        <Button x:Name="SaveButton" Content="Save" Click="SaveButton_Click" Width="50" />
    </StackPanel>
</Window>